<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>IDECodeSnippetCompletionPrefix</key>
	<string>aborted_state</string>
	<key>IDECodeSnippetCompletionScopes</key>
	<array>
		<string>CodeBlock</string>
	</array>
	<key>IDECodeSnippetContents</key>
	<string>aborted_state(&lt;#name#&gt;)
  {
    transition
    {
      if(&lt;#condition#&gt;)
        goto &lt;#state#&gt;;
    }
    action
    {
      &lt;#actions#&gt;
    }
  }</string>
	<key>IDECodeSnippetIdentifier</key>
	<string>B-Human_aborted_state</string>
	<key>IDECodeSnippetLanguage</key>
	<string>Xcode.SourceCodeLanguage.C-Plus-Plus</string>
	<key>IDECodeSnippetSummary</key>
	<string>B-Human: Define aborted state of a CABSL option.</string>
	<key>IDECodeSnippetTitle</key>
	<string>CABSL Aborted State</string>
	<key>IDECodeSnippetUserSnippet</key>
	<true/>
	<key>IDECodeSnippetVersion</key>
	<integer>2</integer>
</dict>
</plist>
